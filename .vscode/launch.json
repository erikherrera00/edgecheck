{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run EdgeCheck (Extension Host)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/vscode-extension"
      ],
      "outFiles": [
        "${workspaceFolder}/vscode-extension/dist/**/*.js"
      ],
      "preLaunchTask": "edgecheck: watch"
    }
  ],
  "compounds": []
}
